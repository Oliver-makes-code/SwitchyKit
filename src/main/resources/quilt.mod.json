{
	"schema_version": 1,
	"quilt_loader": {
		"group": "de.olivermakesco",
		"id": "switchykit",
		"version": "${version}",
		"metadata": {
			"name": "SwitchyKit",
			"description": "A Switchy addon that allows importing system data from PK",
			"contributors": {
				"Octal": "Owner"
			},
			"contact": {
				"issues": "https://github.com/Oliver-makes-code/Switchy-PK/issues",
				"sources": "https://github.com/Oliver-makes-code/Switchy-PK"
			},
			"icon": "assets/switchykit/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "de.olivermakesco.switchykit.SwitchyKit"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=4.0.0-"
			},
			{
				"id": "qkl",
				"versions": ">=0.1.4-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.2"
			},
			{
				"id": "switchy-core",
				"versions": ">=1.8.6"
			},
			{
				"id": "switchy-compat",
				"versions": ">=1.8.6"
			}
		]
	},
	"mixin": "switchykit.mixins.json"
}
